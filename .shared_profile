CONF_DIR=$HOME/.myprofile/conf

source $CONF_DIR/alias
source $CONF_DIR/path
source $CONF_DIR/custom
source $CONF_DIR/ohmyzsh

export PATH=$PATH
export EDITOR=vim
eval "$(~/.local/bin/mise activate bash)"

[ -f "/usr/share/doc/fzf/examples/key-bindings.$(basename ${SHELL})" ] \
&& source "/usr/share/doc/fzf/examples/key-bindings.$(basename ${SHELL})"
